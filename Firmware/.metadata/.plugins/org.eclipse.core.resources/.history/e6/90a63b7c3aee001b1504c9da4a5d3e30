#ifndef INC_APP_TYPES_H_
#define INC_APP_TYPES_H_

#include "main.h"

typedef struct LCD {
	void * clean;
	uint8_t print_sensors_options;
};

typedef struct Buttons {
	uint8_t get_pressed_button;
};

extern struct LCD lcd;
extern struct Buttons buttons;

#endif /* INC_APP_TYPES_H_ */
